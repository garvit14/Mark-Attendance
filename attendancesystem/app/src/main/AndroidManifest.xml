<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.garvitgupta.attendancesystem">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:screenOrientation="portrait"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".BasicInfo"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BasicInfo2"
            android:label="@string/title_activity_basic_info2"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Subjects"
            android:screenOrientation="portrait" />
        <activity
            android:name=".DisplaySubjects"
            android:screenOrientation="portrait" />
        <activity
            android:name=".FeedTimetable"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MarkAttendance"
            android:screenOrientation="portrait" />
        <activity
            android:name=".AttendanceDialog"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Overall"
            android:screenOrientation="portrait" />
        <activity android:name=".GoToDate"></activity>
    </application>

</manifest>